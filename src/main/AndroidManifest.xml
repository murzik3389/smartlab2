        <activity
            android:name="murzik.smartlab.Password"
            android:exported="false" />
        <activity
            android:name="murzik.smartlab.Verification"
            android:exported="false" />
        <activity
            android:name="murzik.smartlab.Auth"
            android:exported="false" />
        <activity
            android:name="murzik.smartlab.Session"
            android:exported="false" />
